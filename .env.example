# Firebase Configuration
# Obtain these values from: https://console.firebase.google.com/
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# App Configuration
VITE_APP_NAME=Projet Blocus
VITE_APP_VERSION=2.0.0
VITE_APP_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:8000

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_SERVICE_WORKER=false

# Gemini AI Configuration (for Firebase Functions)
# This should be set in Firebase Functions Secrets, not here
# Use: firebase functions:secrets:set GEMINI_API_KEY
# GEMINI_API_KEY=your_gemini_api_key
